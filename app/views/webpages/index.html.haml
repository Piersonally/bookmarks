%h1 All Webpages

%table
  %tr
    %th Url

  - for webpage in @webpages
    %tr
      %td=h webpage.url
      %td= link_to 'Show', webpage_path(webpage)
      %td= link_to 'Edit', edit_webpage_path(webpage)
      %td= link_to 'Destroy', webpage_path(webpage), :confirm => 'Are you sure?', :method => :delete
%br
= link_to 'New webpage', new_webpage_path

